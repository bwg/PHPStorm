                <CONSTRUCTOR>true</CONSTRUCTOR>
                <NAME>.*</NAME>
              </AND>
            </match>
            <order>BY_NAME</order>
          </rule>
        </section>
        <section start_comment="// -- Private Methods ------------------------------------------------------">
          <rule>
            <match>
              <AND>
                <METHOD>true</METHOD>
                <NAME>.*</NAME>
                <PRIVATE>true</PRIVATE>
                <STATIC>false</STATIC>
              </AND>
            </match>
            <order>BY_NAME</order>
          </rule>
        </section>
        <section start_comment="// -- Protected Methods ----------------------------------------------------">
